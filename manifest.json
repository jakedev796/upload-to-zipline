{
  "manifest_version": 2,
  "name": "Upload to Zipline",
  "version": "1.3",
  "permissions": [
    "storage",
    "contextMenus",
    "activeTab",
    "downloads",
    "clipboardWrite",
    "notifications",
    "*://*/*"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "options_ui": {
    "page": "options.html",
    "browser_style": true
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "applications": {
    "gecko": {
      "id": "jake@shreves.dev",
      "strict_min_version": "68.0"
    }
  }
}